{
  "manifest_version": 3,
  "name": "Tab Navi",
  "version": "0.0.1",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "description": "Tab manager and navigator.",
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": [
    "https://*/*"
  ],
  "permissions": [
    "tabs",
    "tabGroups"
  ]
}
